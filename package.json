{
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:sub-react-1": "cd packages/sub-react-1 && yarn install",
    "install:sub-react-2": "cd packages/sub-react-2 && yarn install",
    "install:main": "cd packages/main && yarn install",
    "start": "npm-run-all --parallel start:*",
    "start:sub-react-1": "cd packages/sub-react-1 && yarn start",
    "start:sub-react-2": "cd packages/sub-react-2 && yarn start",
    "start:main": "cd packages/main && yarn start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "qiankun": "^2.6.3",
    "npm-run-all": "^4.1.5"
  }
}
